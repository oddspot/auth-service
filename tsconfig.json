{
  "compilerOptions": {
     "target": "es6",
     "module": "commonjs",
     "moduleResolution": "node",
     "outDir": "./dist",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "skipLibCheck": true,
     "removeComments": true,
     "strict": true,
     "noImplicitAny": false,
     "strictNullChecks": true,
     "alwaysStrict": true
  }
}
